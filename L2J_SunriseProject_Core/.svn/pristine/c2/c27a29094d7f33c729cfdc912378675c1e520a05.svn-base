		for (String ID : BLACKLIST)
		{
			try
			{
				MIN_MAX_ABNORMAL_STATE_EFFECT_BLACK_LIST.add(Integer.parseInt(ID));
			}
			catch (NumberFormatException nfe)
			{
				Log.warning(FormulasConfigs.class.getSimpleName() + ": Wrong skill id passed: " + ID);
				Log.warning(nfe.getMessage());
			}
		}
		
		String[] OTHERLIST = getString(_settings, _override, "MinMaxAbonormalStateEffectTimeOtherList", "21;22").trim().split(";");
		MIN_MAX_ABNORMAL_STATE_EFFECT_OTHER_LIST = new ArrayList<>(OTHERLIST.length);
		for (String ID : OTHERLIST)
		{
			try
			{
				MIN_MAX_ABNORMAL_STATE_EFFECT_OTHER_LIST.add(Integer.parseInt(ID));
			}
			catch (NumberFormatException nfe)
			{
				Log.warning(FormulasConfigs.class.getSimpleName() + ": Wrong skill id passed: " + ID);
				Log.warning(nfe.getMessage());
			}
		}
		
		try
		{
			options = getString(_settings, _override, "CapDamagesPerClass", "1,1200-1800;2,2000-2200").split(";");
			
			CAP_DAMAGES_PERCLASS = new HashMap<>(options.length);
			
			// 1,1200-1800
			for (String value : options)
			{
				// 1,1-100
				final String[] values = value.split(",");
				final int classid = Integer.parseInt(values[0]);
				final String[] damages = values[1].split("-");
				
				final int mindamage = Integer.parseInt(damages[0]);
				final int maxdamage = Integer.parseInt(damages[1]);
				
				if (!CAP_DAMAGES_PERCLASS.containsKey(classid))
				{
					CAP_DAMAGES_PERCLASS.put(classid, new HashMap<>());
				}
				
				final Map<Integer, Integer> map = CAP_DAMAGES_PERCLASS.get(classid);
				map.put(mindamage, maxdamage);
			}
		}
		catch (Exception e)
		{
			Log.error("Invalid configuration for CapDamagesPerClass, should be like: CapDamagesPerClass=1,1-100:2-200:3-300:6-550;2,1-100:2-200:3-300:6-550");
		}
	}
	
	public static FormulasConfigs getInstance()
	{
		return SingletonHolder._instance;
	}
	
	private static class SingletonHolder
	{
		protected static final FormulasConfigs _instance = new FormulasConfigs();
	}
}