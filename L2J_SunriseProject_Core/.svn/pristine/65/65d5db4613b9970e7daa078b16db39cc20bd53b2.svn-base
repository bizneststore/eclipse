package gr.sr.voteEngine;

/**
 * @author vGodFather
 */
public class VoteDataReward
{
	private final int _itemId;
	private final int _count;
	private final float _chance;
	
	public VoteDataReward(int itemId, int count, float chance)
	{
		_itemId = itemId;
		_count = count;
		_chance = chance;
	}
	
	public int getId()
	{
		return _itemId;
	}
	
	public int getCount()
	{
		return _count;
	}
	
	public float getChance()
	{
		return _chance;
	}
}
