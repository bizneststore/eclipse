package gr.sr.advancedBuffer;

import java.util.ArrayList;
import java.util.List;

public class PlayerScheme
{
	public final int schemeId;
	public String schemeName;
	public int iconId;
	public final List<SchemeBuff> schemeBuffs;
	
	protected PlayerScheme(int schemeId, String schemeName, int iconId)
	{
		this.schemeId = schemeId;
		this.schemeName = schemeName;
		this.iconId = iconId;
		schemeBuffs = new ArrayList<>();
	}
}